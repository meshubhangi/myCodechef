#include <stdio.h>
 
int main(void)
{
    int n, sum=0, t, b, i;
    scanf("%d", &n);
    for(i=0; i<n; i++)
    {
        scanf("%d", &t);
        sum=0;
        while(t>0)
        { 
            b=t%10;
            sum=sum+b;
            t=t/10;
        }
        printf("%d \n", sum);
    }
	return 0;
}